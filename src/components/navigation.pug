.layout-transparent.mdl-layout.mdl-js-layout
header.mdl-layout__header.mdl-layout__header--transparent
    .mdl-layout__header-row.mdl-shadow--4dp
        span.mdl-layout-title KIIT
        .mdl-layout-spacer
        nav.mdl-navigation
            a.mdl-navigation__link(onclick='return page_nav("home")') Home
            a.mdl-navigation__link(onclick='return page_nav("aboutus")') About Us
            a.mdl-navigation__link(href='') Author
            a.mdl-navigation__link(onclick='return page_nav("register")') Registration
            a.mdl-navigation__link(onclick='return page_nav("committee")') Speakers
            a.mdl-navigation__link(onclick='return page_nav("committee")') Committee
            a.mdl-navigation__link(href='') Travel
            a.mdl-navigation__link(href='') Contact Us
        main.mdl-layout__content
ul.mdl-menu.mdl-menu--bottom-left.mdl-js-menu.mdl-js-ripple-effect(for='demo-menu-lower-left')
    li.mdl-menu__item Some Action
    li.mdl-menu__item.mdl-menu__item--full-bleed-divider Another Action
    li.mdl-menu__item(disabled='') Disabled Action
    li.mdl-menu__item Yet Another Action
script.
    function page_nav(page) {
        $(".page").hide();
        $("#page_" + page).show();
    }